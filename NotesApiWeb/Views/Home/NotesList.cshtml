@model IEnumerable<NotesApi.Models.note>



@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userToken)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.note1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lastModified)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.noteId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.noteId })
        </td>
    </tr>
}